<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_2.xsd">
	<persistence-unit name="AppRondas_Grupo04" transaction-type="RESOURCE_LOCAL">
		<class>projetos.AppRondas_Grupo04.domain.Usuario</class>
		<class>projetos.AppRondas_Grupo04.domain.Pessoa</class>
		<class>projetos.AppRondas_Grupo04.domain.Ronda</class>
		<class>projetos.AppRondas_Grupo04.domain.Locomocao</class>
		<class>projetos.AppRondas_Grupo04.domain.Localizacao</class>
		<class>projetos.AppRondas_Grupo04.domain.Ocorrencia</class>
		<properties>
			<property name="javax.persistence.jdbc.url" value="jdbc:postgresql://localhost:5432/AppRondas_Grupo04"/>
			<property name="javax.persistence.jdbc.user" value="postgres"/>
			<property name="javax.persistence.jdbc.password" value="masterkey"/>
			<property name="javax.persistence.jdbc.driver" value="org.postgresql.Driver"/>
			<property name="javax.persistence.schema-generation.database.action" value="create"/>
		</properties>
	</persistence-unit>
</persistence>
